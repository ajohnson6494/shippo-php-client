{
  "name": "ajohnson6494/shippo-php",
  "description": "A PHP library for connecting with multiple carriers (FedEx, UPS, USPS) using Shippo.",
  "keywords": [
    "ups",
    "usps",
    "fedex",
    "dhl",
    "uber",
    "shyp",
    "shipping",
    "tracking",
    "address"
  ],
  "homepage": "https://goshippo.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Shippo & Contributors",
      "homepage": "https://goshippo.com/"
    },
    {
      "name": "Andrew Donald Johnson"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "Shippo\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  }
}
